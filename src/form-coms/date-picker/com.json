{
  "title": "日期选择框",
  "namespace": "mybricks.normal-pc.date-picker",
  "author": "HuangQiuyun",
  "author_name": "黄秋云",
  "version": "1.1.7",
  "description": "日期选择框",
  "icon": "./icon.svg",
  "preview": "https://f2.eckwai.com/kos/nlav11092/u_taeb6n.f449835eb18b532c.png",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setValue",
      "title": "设置值",
      "desc": "设置日期选择的值，并触发值变化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setInitialValue",
      "title": "设置初始值",
      "desc": "设置日期选择的初始值，并触发初始化事件输出",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "validate",
      "title": "校验",
      "desc": "触发日期选择的校验检查，并输出校验结果",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValidate"
      ]
    },
    {
      "id": "getValue",
      "title": "获取值",
      "desc": "获取日期选择的值并输出",
      "schema": {
        "type": "any"
      },
      "rels": [
        "returnValue"
      ]
    },
    {
      "id": "resetValue",
      "title": "重置值",
      "desc": "清空日期选择的值",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDisabled",
      "title": "设置禁用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setEnabled",
      "title": "设置启用",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setValidateInfo",
      "title": "设置校验状态",
      "desc": "设置表单项的校验状态和校验失败提示文案",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "type": "enum",
            "items": [
              {
                "type": "string",
                "value": "success"
              },
              {
                "type": "string",
                "value": "error"
              }
            ]
          },
          "help": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "disabledDate",
      "title": "禁用特定日期",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setColor",
      "title": "设置字体颜色",
      "desc": "设置日期选择框里字体的颜色",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "setDateType",
      "title": "设置日期选择类型",
      "desc": "设置日期选择类型",
      "schema": {
        "type": "enum",
        "items": [
          {
            "type": "string",
            "value": "date"
          },
          {
            "type": "string",
            "value": "week"
          },
          {
            "type": "string",
            "value": "month"
          },
          {
            "type": "string",
            "value": "quarter"
          },
          {
            "type": "string",
            "value": "year"
          }
        ]
      }
    }
  ],
  "outputs": [
    {
      "id": "onInitial",
      "title": "值初始化",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onChange",
      "title": "值更新",
      "schema": {
        "type": "string"
      }
    },
    {
      "id": "onValidate",
      "title": "校验触发",
      "schema": {
        "title": "当前值",
        "type": "string"
      }
    },
    {
      "id": "returnValidate",
      "title": "校验结果",
      "schema": {
        "type": "object",
        "properties": {
          "validateStatus": {
            "title": "校验状态",
            "type": "string"
          },
          "help": {
            "title": "校验提示",
            "type": "string"
          }
        }
      }
    },
    {
      "id": "returnValue",
      "title": "值输出",
      "schema": {
        "type": "string"
      }
    }
  ],
  "schema": [
    "mybricks.normal-pc.form-container/form-item",
    "*"
  ]
}