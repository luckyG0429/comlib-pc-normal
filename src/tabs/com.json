{
  "title": "标签页",
  "namespace": "mybricks.normal-pc.tabs",
  "version": "1.0.7",
  "description": "选项卡切换组件",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editor/index.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "previous",
      "title": "上一页",
      "schema": {
        "type": "any"
      },
      "desc": "标签页上一页"
    },
    {
      "id": "next",
      "title": "下一页",
      "schema": {
        "type": "any"
      },
      "desc": "标签页下一页"
    },
    {
      "id": "active",
      "title": "激活tab项",
      "schema": {
        "type": "number"
      },
      "desc": "标签页下标，从0开始计数"
    },
    {
      "id": "outActiveTab",
      "title": "获取激活tab项",
      "rels": [
        "outActiveTab"
      ],
      "schema": {
        "type": "any"
      },
      "desc": "获取当前显示的标签页信息"
    }
  ],
  "outputs": [
    {
      "id": "tabClick",
      "title": "标签页点击",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "title": "标签页id",
            "type": "string"
          },
          "name": {
            "title": "标签页标题",
            "type": "string"
          },
          "key": {
            "title": "标签页key",
            "type": "string"
          },
          "index": {
            "title": "下标",
            "type": "number"
          }
        }
      }
    },
    {
      "id": "outActiveTab",
      "title": "激活tab项输出",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "title": "标签页id",
            "type": "string"
          },
          "name": {
            "title": "标签页标题",
            "type": "string"
          },
          "key": {
            "title": "标签页key",
            "type": "string"
          },
          "index": {
            "title": "下标",
            "type": "number"
          }
        }
      }
    },
    {
      "id": "tab0_into",
      "title": "标签页1显示",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "tab0_leave",
      "title": "标签页1隐藏",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": [
    {
      "id": "tab0",
      "title": "标签页1"
    }
  ]
}