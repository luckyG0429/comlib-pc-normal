{
  "title": "树",
  "namespace": "mybricks.normal-pc.tree",
  "version": "1.0.33",
  "description": "树状图",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "treeData",
      "title": "输入数据",
      "schema": {
        "title": "树组件数据",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "title": "标题",
              "type": "string"
            },
            "key": {
              "title": "字段名",
              "type": "string"
            },
            "disableCheckbox": {
              "title": "禁用勾选",
              "type": "boolean"
            },
            "children": {
              "title": "子项",
              "type": "array",
              "items": {
                "type": "object"
              }
            }
          }
        }
      }
    },
    {
      "id": "nodeData",
      "title": "更新节点数据",
      "schema": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "children": {
            "title": "子项",
            "type": "array",
            "items": {
              "type": "object"
            }
          }
        }
      }
    },
    {
      "id": "searchValue",
      "title": "搜索",
      "schema": {
        "type": "string"
      },
      "desc": "输入字符串，高亮展示匹配的树节点"
    },
    {
      "id": "filter",
      "title": "过滤",
      "schema": {
        "type": "string"
      },
      "desc": "输入字符串，筛选匹配的树节点"
    },
    {
      "id": "setSelectedKeys",
      "title": "设置选中项",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "desc": "设置树形结构中选中的节点"
    },
    {
      "id": "checkedValues",
      "title": "设置勾选项",
      "schema": {
        "type": "array",
        "items": {
          "title": "字段名",
          "type": "string"
        }
      }
    },
    {
      "id": "disableCheckbox",
      "title": "禁用勾选",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "enableCheckbox",
      "title": "启用勾选",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "submit",
      "title": "获取勾选项数据",
      "rels": [
        "submit"
      ],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDragConfig",
      "title": "设置拖拽功能",
      "schema": {
        "type": "object",
        "properties": {
          "draggable": {
            "type": "enum",
            "items": [
              {
                "type": "boolean"
              },
              {
                "type": "string",
                "value": "custom"
              }
            ]
          },
          "draggableScript": {
            "type": "string"
          },
          "allowDrop": {
            "type": "enum",
            "items": [
              {
                "type": "boolean"
              },
              {
                "type": "string",
                "value": "custom"
              }
            ]
          },
          "allowDropScript": {
            "type": "string"
          },
          "useDropScope": {
            "type": "enum",
            "items": [
              {
                "type": "boolean",
                "value": false
              },
              {
                "type": "string",
                "value": "parent"
              }
            ]
          },
          "dropScopeMessage": {
            "type": "string"
          }
        }
      }
    },
    {
      "id": "setOpenDepth",
      "title": "设置展开深度",
      "desc": "设置树的展开深度: 0表示全部折叠, -1表示全部展开",
      "schema": {
        "type": "number"
      }
    },
    {
      "id": "addTips",
      "title": "设置添加提示文案",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "submit",
      "title": "勾选项数据输出",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "click",
      "title": "节点选中事件",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    {
      "id": "addNodeDone",
      "title": "添加完成",
      "schema": {
        "type": "object",
        "properties": {
          "node": {
            "type": "object",
            "title": "新增节点数据",
            "properties": {
              "title": {
                "type": "string"
              },
              "value": {
                "type": "string"
              },
              "key": {
                "type": "string"
              }
            }
          },
          "parent": {
            "type": "object",
            "title": "新增节点的父节点数据"
          }
        }
      }
    },
    {
      "id": "modify",
      "title": "修改",
      "schema": {
        "title": "节点数据",
        "type": "object",
        "properties": {
          "title": {
            "title": "标题",
            "type": "string"
          },
          "key": {
            "title": "字段名",
            "type": "string"
          },
          "value": {
            "title": "值",
            "type": "string"
          },
          "disableCheckbox": {
            "title": "禁用勾选",
            "type": "boolean"
          }
        }
      }
    },
    {
      "id": "delete",
      "title": "删除",
      "schema": {
        "title": "节点数据",
        "type": "object",
        "properties": {
          "title": {
            "title": "标题",
            "type": "string"
          },
          "key": {
            "title": "字段名",
            "type": "string"
          },
          "value": {
            "title": "值",
            "type": "string"
          },
          "disableCheckbox": {
            "title": "禁用勾选",
            "type": "boolean"
          }
        }
      }
    },
    {
      "id": "onDropDone",
      "title": "拖拽完成",
      "schema": {
        "type": "object"
      }
    }
  ]
}