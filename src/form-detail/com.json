{
  "title": "描述列表",
  "namespace": "mybricks.normal-pc.form-detail",
  "version": "1.0.22",
  "description": "描述列表",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.tsx",
  "upgrade": "./upgrade.ts",
  "target": {
    "toReact": "./toReact.ts"
  },
  "inputs": [
    {
      "id": "setTitle",
      "title": "设置标题",
      "desc": "设置描述列表的标题",
      "schema": {
        "type": "string"
      },
      "rels": [
        "setTitleComplete"
      ]
    },
    {
      "id": "setDataSource",
      "title": "设置数据源",
      "desc": "设置描述列表的数据源",
      "schema": {
        "type": "object",
        "properties": {
          "field1": {
            "type": "string"
          }
        }
      },
      "rels": [
        "setDataSourceComplete"
      ]
    },
    {
      "id": "setDataDesc",
      "title": "设置数据源描述",
      "desc": "设置数据源描述",
      "schema": {
        "type": "object",
        "properties": {
          "field1": {
            "type": "object",
            "properties": {
              "label": {
                "type": "string"
              },
              "labelDesc": {
                "type": "string"
              },
              "showLabel": {
                "type": "boolean"
              },
              "visible": {
                "type": "boolean"
              }
            }
          }
        }
      },
      "rels": [
        "setDataDescComplete"
      ]
    }
  ],
  "outputs": [
    {
      "id": "setTitleComplete",
      "desc": "设置描述列表的标题后，会触发描述列表的设置标题完成输出",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDataSourceComplete",
      "desc": "设置描述列表的数据源后，会触发设置数据源完成输出",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "setDataDescComplete",
      "desc": "设置描述列表的数据源描述后，触发设置数据源描述完成事件输出",
      "title": "完成",
      "schema": {
        "type": "any"
      }
    }
  ],
  "slots": []
}