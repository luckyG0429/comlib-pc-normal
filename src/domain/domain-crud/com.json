{
  "title": "领域模型CRUD",
  "namespace": "mybricks.domain-pc.crud",
  "version": "1.0.3",
  "description": "领域模型CRUD",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors/index.tsx",
  "inputs": [
    {
      "id": "query",
      "title": "查询记录",
      "desc": "根据输入条件查询记录",
      "schema": {
        "type": "object",
        "properties": {}
      }
    }
  ],
  "outputs": [],
  "slots": [
    {
      "id": "queryContent",
      "title": "查询区",
      "schema": "mybricks.domain-pc.crud/query",
      "capacity": 1,
      "type": "scope",
      "_inputs": [
        {
          "id": "onSubmit",
          "title": "查询",
          "schema": {
            "type": "object",
            "properties": {}
          }
        }
      ]
    },
    {
      "id": "actionsContent",
      "title": "操作区"
    },
    {
      "id": "tableContent",
      "title": "列表展示区",
      "schema": "mybricks.domain-pc.crud/table",
      "capacity": 1,
      "type": "scope",
      "_inputs": [
        {
          "id": "onPageChange",
          "title": "触发分页变化",
          "schema": {
            "type": "object",
            "properties": {
              "pageNum": {
                "title": "页码",
                "type": "number"
              },
              "pageSize": {
                "title": "每页条数",
                "type": "number"
              }
            }
          }
        },
        {
          "id": "onSorterChange",
          "title": "触发排序变化",
          "schema": {
            "type": "object",
            "properties": {
              "field": {
                "title": "排序字段",
                "type": "string"
              },
              "sorter": {
                "title": "排序方式",
                "type": "string"
              }
            }
          }
        }
      ]
    }
  ]
}
